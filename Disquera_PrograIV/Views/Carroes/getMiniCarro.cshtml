@model IEnumerable<Disquera_PrograIV.Models.Carro>
@{
    ViewBag.Title = "Carro de Compras";
}
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Disco.dis_alb)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.car_can)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Disco.dis_alb)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.car_can)
            </td>
            <td><img src="img/delete.png" width="24" height="24" onclick="carrocompras_eliminar(@item.dis_id)" /></td>
        </tr>
    }

</table>
