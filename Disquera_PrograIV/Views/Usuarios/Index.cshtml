@model IEnumerable<Disquera_PrograIV.Models.Usuario>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.usu_nom)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.usu_ape)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AspNetUsers.Email)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.usu_nom)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.usu_ape)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AspNetUsers.Email)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.usu_rut }) |
            @Html.ActionLink("Details", "Details", new { id=item.usu_rut }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.usu_rut })
        </td>
    </tr>
}

</table>
